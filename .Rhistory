library(ggplot2)
# Importing the data set and printing the first 6 rows to see what it looks like
asthmaDataOriginal = read.csv("Asthma_Data_File.csv") %>%
rename(subj = UserNo.,
loc = Location,
age = Age,
gender = Gender,
odj = OutdoorJob,
oda = OutdoorActivities,
sh = SmokingHabit,
hum = Humidity,
pres = Pressure,
temp = Temperature,
uv = UVIndex,
ws = WindSpeed,
score = ACTScore)
